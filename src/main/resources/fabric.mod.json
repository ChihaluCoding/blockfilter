{
	"schemaVersion": 1,
	"id": "blockfilter",
	"version": "${version}",
	"name": "BlockFilter",
	"description": "Adds filter-friendly creative tabs including Recent Blocks and Biome collections.",
	"authors": [
		"chihalu"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/blockfilter/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"chihalu.blockfilter.BlockFilter"
		],
		"client": [
			"chihalu.blockfilter.BlockFilterClient"
		],
		"fabric-datagen": [
			"chihalu.blockfilter.BlockFilterDataGenerator"
		]
	},
	"mixins": [
		"blockfilter.mixins.json",
		{
			"config": "blockfilter.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.9",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
